{
  "name": "havename",
  "displayName": "HaveName",
  "description": "I have a name",
  "publisher": "havename",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.73.0"
  },
  "icon": "logo.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:javascript",
    "onLanguage:typescriptreact",
    "onLanguage:javascriptreact"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "HaveName",
      "type": "object",
      "properties": {
        "vscodeFanyi.Apifrom": {
          "type": "string",
          "default": "YD",
          "enum": [
            "YD",
            "BD"
          ],
          "enumDescriptions": [
            "使用有道API",
            "使用百度API"
          ]
        },
        "vscodeFanyi.Apikey": {
          "type": "string",
          "description": "密钥"
        },
        "vscodeFanyi.Apiname": {
          "type": "string",
          "description": "应用ID"
        },
        "vscodeFanyi.time": {
          "type": "number",
          "default": 500,
          "description": "输入防抖"
        },
        "vscodeFanyi.validLanguages": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "description": "设置扩展在这些语言上运行",
          "default": [
            "vue",
            "typescript",
            "javascript",
            "typescriptreact",
            "javascriptreact"
          ]
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ohzsh/havename"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@types/crypto-js": "^4.1.1",
    "@types/glob": "^8.0.0",
    "@types/md5": "^2.3.2",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/qs": "^6.9.7",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "axios": "^1.2.0",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "youdao-fanyi": "^2.0.0"
  }
}
